{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@components/*": [
                "./src/components/*"
            ],
            "@assets/*": [
                "./src/assets/*"
            ],
            "@pages/*": [
                "./src/pages/*"
            ],
            "@layout/*": [
                "/src/layout/*"
            ],
            "@routes/*": [
                "/src/routes/*"
            ],
            "@/*": [
                "./src/*"
            ],
            "@utils/*": [
                "./src/utils/*"
            ]
        }
    }
}